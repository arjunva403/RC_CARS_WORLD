                        <div class="detail-value">
                            <%= order.shippingAddress.addressType %>
                        </div>
                    </div>
                    <div class="detail-item">
                        <div class="detail-label">Payment Method</div>
                        <div class="detail-value">
                            <%= order.paymentMethod %>
                        </div>
                    </div>
                    <div class="detail-item">
                        <div class="detail-label">Shipping Address</div>
                        <div class="detail-value">
                            <%= order.shippingAddress.address %><br>
                                <%= order.shippingAddress.landmark %><br>
                                    <%= order.shippingAddress.city %>, <%= order.shippingAddress.state %><br>
                                            <%= order.shippingAddress.pincode %>, India
                        </div>
                    </div>
                </div>
            </section>

            <!-- Enhanced Action Buttons -->
            <div class="action-buttons">
                <button class="btn btn-primary" onclick="window.print()">
                    <i class="fas fa-print"></i>
                    Print Invoice
                </button>
                <button class="btn btn-outline-primary" onclick="exportOrderData()">
                    <i class="fas fa-download"></i>
                    Export Order Data
                </button>
                <button class="btn btn-outline-primary" onclick="window.history.back()">
                    <i class="fas fa-arrow-left"></i>
                    Back to Orders
                </button>
            </div>
        </div>
    </main>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"></script>
    <script>
        // Global variables
        let selectedItems = [];

        document.addEventListener('DOMContentLoaded', function () {
            // Add loading animation on page load
            const sections = document.querySelectorAll('.order-header-card, .stats-section, .content-section');
            sections.forEach((section, index) => {
                section.style.opacity = '0';
                section.style.transform = 'translateY(20px)';
                setTimeout(() => {
                    section.style.transition = 'all 0.6s ease';
                    section.style.opacity = '1';
                    section.style.transform = 'translateY(0)';
                }, index * 100);
            });

            // Add event listeners to item checkboxes
            document.querySelectorAll('.item-checkbox').forEach(cb => {
                cb.addEventListener('change', updateSelectedItems);
            });
        });

        // Enhanced Message Functions
        function showSuccessMessage(title, message) {
            Swal.fire({
                title: `<strong><i class="fas fa-check-circle"></i> ${title}</strong>`,
                html: `<div style="font-size:1.1rem;margin-top:10px;line-height:1.6;">${message}</div>`,
                icon: 'success',
                confirmButtonText: '<i class="fas fa-thumbs-up"></i> Excellent!',
                timer: 4000,
                timerProgressBar: true,
                showClass: {
                    popup: 'swal2-show',
                    backdrop: 'swal2-backdrop-show',
                    icon: 'swal2-icon-show'
                },
                hideClass: {
                    popup: 'swal2-hide',
                    backdrop: 'swal2-backdrop-hide',
                    icon: 'swal2-icon-hide'
                }
            });
        }

        function showErrorMessage(title, message) {
            Swal.fire({
                title: `<strong><i class="fas fa-exclamation-triangle"></i> ${title}</strong>`,
                html: `<div style="font-size:1.1rem;margin-top:10px;line-height:1.6;">${message}</div>`,
                icon: 'error',
                confirmButtonText: '<i class="fas fa-redo"></i> Try Again',
                showClass: {
                    popup: 'swal2-show',
                    backdrop: 'swal2-backdrop-show',
                    icon: 'swal2-icon-show'
                }
            });
        }

        function showLoadingMessage(title, message) {
            Swal.fire({
                title: `<strong><i class="fas fa-spinner fa-spin"></i> ${title}</strong>`,
                html: `<div style="font-size:1.1rem;margin-top:10px;line-height:1.6;">${message}</div>`,
                showConfirmButton: false,
                allowOutsideClick: false,
                allowEscapeKey: false,
                didOpen: () => {
                    Swal.showLoading();
                }
            });
        }

        // Bulk Operations
        function toggleSelectAll() {
            const selectAll = document.getElementById('selectAllItems');
            const checkboxes = document.querySelectorAll('.item-checkbox');

            checkboxes.forEach(cb => cb.checked = selectAll.checked);
            updateSelectedItems();
        }

        function updateSelectedItems() {
            selectedItems = Array.from(document.querySelectorAll('.item-checkbox:checked'))
                .map(cb => cb.dataset.itemId);

            const selectAll = document.getElementById('selectAllItems');
            const allCheckboxes = document.querySelectorAll('.item-checkbox');
            const bulkPanel = document.getElementById('bulkActionsPanel');
            const selectedCount = document.getElementById('selectedCount');

            selectAll.checked = selectedItems.length === allCheckboxes.length;
            selectAll.indeterminate = selectedItems.length > 0 && selectedItems.length < allCheckboxes.length;

            // Show/hide bulk actions panel
            if (selectedItems.length > 0) {
                bulkPanel.classList.add('show');
                selectedCount.textContent = selectedItems.length;
            } else {
                bulkPanel.classList.remove('show');
            }
        }

        // Enhanced Return Reason Validation Function
        async function validateReturnReason(itemId, productName, returnReason, orderId, returnQty) {
            const result = await Swal.fire({
                title: `<strong><i class="fas fa-clipboard-check"></i> Return Reason Validation</strong>`,
                html: `
                    <div style="text-align:left;margin-top:1rem;">
                        <p style="margin-bottom:1rem;"><strong><i class="fas fa-box"></i> Product:</strong> ${productName}</p>
                       
                        <div class="reason-display-box">
                            <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-comment-alt"></i> Customer Return Reason:</strong></p>
                            <p class="reason-text">${returnReason}</p>
                             <p style="margin-bottom:1rem;"><strong>ReturnQty:</strong> ${returnQty}</p>
                        </div>
                        <p style="margin-bottom:0;color: rgba(255, 255, 255, 0.8);"><i class="fas fa-info-circle"></i> Please review the return reason and decide whether to accept or reject this return request.</p>
                    </div>
                `,
                width: '600px',
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-check"></i> Accept Return',
                cancelButtonText: '<i class="fas fa-times"></i> Reject Return',
                reverseButtons: true,
                customClass: {
                    popup: 'reason-validation-modal'
                },
                showDenyButton: false,
                allowOutsideClick: false
            });

            if (result.isConfirmed) {
                await processReturnValidation('Accept', itemId, orderId, productName, returnReason, returnQty);
            } else if (result.isDismissed && result.dismiss === Swal.DismissReason.cancel) {
                await processReturnValidation('Reject', itemId, orderId, productName, returnReason, returnQty);
            }
        }

        // Enhanced Return Request Processing with Validation
        async function processReturnValidation(action, itemId, orderId, productName, returnReason, returnQty) {
            showLoadingMessage(`${action}ing Return Request`, `Processing return validation for ${productName}...`);

            try {
                const response = await fetch('/returnStatus', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        action: action,
                        itemId: itemId,
                        orderId: orderId,
                        productName: productName,
                        returnReason: returnReason,
                        returnQty: returnQty,
                        adminDecision: action
                    })
                });

                const data = await response.json();

                if (data.success) {
                    showSuccessMessage(
                        `Return Request ${action}ed!`,
                        `${productName} return request has been ${action.toLowerCase()}ed successfully. ${action === 'Accept' ? 'Refund will be processed within 3-5 business days.' : 'Customer has been notified about the rejection.'}`
                    );
                    setTimeout(() => location.reload(), 2000);
                } else {
                    showErrorMessage('Processing Failed', data.message || `Failed to ${action.toLowerCase()} return request`);
                }
            } catch (error) {
                console.error('Return validation error:', error);
                showErrorMessage('Network Error', 'Unable to process request. Please try again.');
            }
        }


        async function processReturnRequest(action, itemId, orderId, productName) {
            showLoadingMessage(`${action}ing Return Request`, `Processing return request for ${productName}...`);

            try {
                const response = await fetch('/admin/order/process-return', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        action: action,
                        itemId: itemId,
                        orderId: orderId
                    })
                });

                const data = await response.json();

                if (data.success) {
                    showSuccessMessage(
                        `Return Request ${action}ed!`,
                        `${productName} return request has been ${action.toLowerCase()}ed successfully.`
                    );
                    setTimeout(() => location.reload(), 2000);
                } else {
                    showErrorMessage('Processing Failed', data.message || `Failed to ${action.toLowerCase()} return request`);
                }
            } catch (error) {
                showErrorMessage('Network Error', 'Unable to process request. Please try again.');
            }
        }

        async function markAsShipped(itemId, productName) {
            const result = await Swal.fire({
                title: `<strong><i class="fas fa-shipping-fast"></i> Mark as Shipped</strong>`,
                html: `<div style="text-align:left;margin-top:1rem;">
                        <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-box"></i> Product:</strong> ${productName}</p>
                        <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-truck"></i> Action:</strong> Mark item as shipped</p>
                        <p style="margin-bottom:0;"><strong><i class="fas fa-bell"></i> Notification:</strong> Customer will receive tracking information</p>
                       </div>`,
                icon: 'question',
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-shipping-fast"></i> Mark as Shipped',
                cancelButtonText: '<i class="fas fa-times"></i> Cancel'
            });

            if (result.isConfirmed) {
                await updateItemStatusAPI(itemId, 'Shipped', productName);
            }
        }

        async function markAsDelivered(itemId, productName) {
            const result = await Swal.fire({
                title: `<strong><i class="fas fa-check-circle"></i> Mark as Delivered</strong>`,
                html: `<div style="text-align:left;margin-top:1rem;">
                        <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-box"></i> Product:</strong> ${productName}</p>
                        <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-check-circle"></i> Action:</strong> Mark item as delivered</p>
                        <p style="margin-bottom:0;"><strong><i class="fas fa-star"></i> Result:</strong> Order completion and review request sent</p>
                       </div>`,
                icon: 'success',
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-check-circle"></i> Mark as Delivered',
                cancelButtonText: '<i class="fas fa-times"></i> Cancel'
            });

            if (result.isConfirmed) {
                await updateItemStatusAPI(itemId, 'Delivered', productName);
            }
        }

        async function updateItemStatusAPI(itemId, status, productName) {
            showLoadingMessage('Updating Status', `Updating ${productName} status to ${status}...`);

            try {
                const response = await fetch('/admin/order/update-item-status', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        itemId: itemId,
                        status: status,
                        orderId: '<%= order._id %>'
                    })
                });

                const data = await response.json();

                if (data.success) {
                    showSuccessMessage('Status Updated!', `${productName} has been marked as ${status.toLowerCase()}`);
                    setTimeout(() => location.reload(), 2000);
                } else {
                    showErrorMessage('Update Failed', data.message);
                }
            } catch (error) {
                showErrorMessage('Network Error', 'Failed to update status');
            }
        }

        async function bulkUpdateSelected() {
            if (selectedItems.length === 0) {
                showErrorMessage('No Items Selected', 'Please select items to update');
                return;
            }

            const { value: newStatus } = await Swal.fire({
                title: `<strong><i class="fas fa-tasks"></i> Bulk Status Update</strong>`,
                html: `<p>Update status for <strong>${selectedItems.length}</strong> selected items:</p>`,
                input: 'select',
                inputOptions: {
                    'Processing': '⚙️ Processing',
                    'Shipped': '🚚 Shipped',
                    'Delivered': '📦 Delivered'
                },
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-save"></i> Update All',
                inputValidator: (value) => !value && 'Please select a status!'
            });

            if (newStatus) {
                showLoadingMessage('Bulk Update', `Updating ${selectedItems.length} items...`);

                try {
                    const response = await fetch('/admin/order/bulk-update-status', {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({
                            itemIds: selectedItems,
                            status: newStatus,
                            orderId: '<%= order._id %>'
                        })
                    });

                    const data = await response.json();

                    if (data.success) {
                        showSuccessMessage('Bulk Update Complete!', `${selectedItems.length} items updated to ${newStatus}`);
                        setTimeout(() => location.reload(), 2000);
                    } else {
                        showErrorMessage('Bulk Update Failed', data.message);
                    }
                } catch (error) {
                    showErrorMessage('Network Error', 'Failed to process bulk update');
                }
            }
        }

        async function bulkMarkShipped() {
            if (selectedItems.length === 0) {
                showErrorMessage('No Items Selected', 'Please select items to mark as shipped');
                return;
            }

            const result = await Swal.fire({
                title: `<strong><i class="fas fa-shipping-fast"></i> Bulk Mark as Shipped</strong>`,
                html: `<p>Mark <strong>${selectedItems.length}</strong> selected items as shipped?</p>`,
                icon: 'question',
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-shipping-fast"></i> Mark All as Shipped',
                cancelButtonText: '<i class="fas fa-times"></i> Cancel'
            });

            if (result.isConfirmed) {
                await bulkUpdateStatusAPI(selectedItems, 'Shipped');
            }
        }

        async function bulkMarkDelivered() {
            if (selectedItems.length === 0) {
                showErrorMessage('No Items Selected', 'Please select items to mark as delivered');
                return;
            }

            const result = await Swal.fire({
                title: `<strong><i class="fas fa-check-circle"></i> Bulk Mark as Delivered</strong>`,
                html: `<p>Mark <strong>${selectedItems.length}</strong> selected items as delivered?</p>`,
                icon: 'success',
                showCancelButton: true,
                confirmButtonText: '<i class="fas fa-check-circle"></i> Mark All as Delivered',
                cancelButtonText: '<i class="fas fa-times"></i> Cancel'
            });

            if (result.isConfirmed) {
                await bulkUpdateStatusAPI(selectedItems, 'Delivered');
            }
        }

        async function bulkUpdateStatusAPI(itemIds, status) {
            showLoadingMessage('Bulk Update', `Updating ${itemIds.length} items to ${status}...`);

            try {
                const response = await fetch('/admin/order/bulk-update-status', {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        itemIds: itemIds,
                        status: status,
                        orderId: '<%= order._id %>'
                    })
                });

                const data = await response.json();

                if (data.success) {
                    showSuccessMessage('Bulk Update Complete!', `${itemIds.length} items updated to ${status}`);
                    setTimeout(() => location.reload(), 2000);
                } else {
                    showErrorMessage('Bulk Update Failed', data.message);
                }
            } catch (error) {
                showErrorMessage('Network Error', 'Failed to process bulk update');
            }
        }

        async function showItemReason(itemId, productName, orderId, cancellationreason, cancellationQty) {
            const result = await Swal.fire({
                title: `<strong><i class="fas fa-clipboard-check"></i> Cancellation Reason Validation</strong>`,
                html: `
            <div style="text-align:left;margin-top:1rem;">
                <p style="margin-bottom:1rem;"><strong><i class="fas fa-box"></i> Product:</strong> ${productName}</p>
               
                <div class="reason-display-box">
                    <p style="margin-bottom:0.5rem;"><strong><i class="fas fa-comment-alt"></i> Customer Cancellation Reason:</strong></p>
                    <p class="reason-text">${cancellationreason}</p>
                     <p style="margin-bottom:1rem;"><strong>Cancelled Qty:</strong> ${cancellationQty}</p>
                </div>
               
            </div>
        `,
                width: '600px',

            });

           
        }
       
        function exportOrderData() {
            showSuccessMessage('Export Started!', 'Order data export has been initiated. Download will begin shortly.');
            // Add actual export functionality here
        }
    </script>
