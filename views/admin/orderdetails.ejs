    <!-- Enhanced Main Content -->
    <main class="main-content">
        <!-- Enhanced Top Bar -->
        <header class="topbar">
            <div class="topbar-left">
                <h1>Order Details</h1>
                <div class="breadcrumb">
                    <a href="/order"><i class="fas fa-shopping-cart"></i> Orders</a>
                    <i class="fas fa-chevron-right"></i>
                    <span>Order Details</span>
                </div>
            </div>
            <div class="topbar-right">
                <div class="user-info">
                    <i class="fas fa-user-circle"></i>
                    <span>Welcome, Admin</span>
                </div>
                <div class="user-info">
                    <i class="fas fa-cog"></i>
                </div>
                <button class="logout-btn" onclick="window.location.href='/logout'">
                    <i class="fas fa-sign-out-alt"></i>
                    <span>Logout</span>
                </button>
            </div>
        </header>

        <!-- Enhanced Content -->
        <div class="content">
            <!-- Page Header -->
            <div class="page-header">
                <h2 class="page-title">Order Overview</h2>
                <p class="page-subtitle">Complete details and management options for this order</p>
            </div>

            <a href="/order" class="back-btn">
                <i class="fas fa-arrow-left"></i>
                <span>Back to Orders</span>
            </a>

            <!-- Order Header Card -->
            <section class="order-header-card">
                <div class="order-id-display">Order ID: #ORD-<%= order.orderId %>
                </div>
                <div class="order-date">
                    Placed on <%= new Date(order.createdOn).toLocaleDateString("en-IN", { year: 'numeric' ,
                        month: 'short' , day: 'numeric' }) %>
                </div>
            </section>

            <!-- Enhanced Stats Section -->
            <section class="stats-section">
                <div class="stats-grid">
                    <div class="stat-card">
                        <div class="stat-header">
                            <div class="stat-title">Total Items</div>
                            <div class="stat-icon" style="background: rgba(231, 76, 60, 0.1); color: var(--primary);">
                                <i class="fas fa-box"></i>
                            </div>
                        </div>
                        <div class="stat-value">
                            <%= order.orderitems.length %>
                        </div>
                    </div>

                    <div class="stat-card">
                        <div class="stat-header">
                            <div class="stat-title">Total Quantity</div>
                            <div class="stat-icon" style="background: rgba(255, 193, 7, 0.1); color: var(--warning);">
                                <i class="fas fa-layer-group"></i>
                            </div>
                        </div>
                        <div class="stat-value" id="totalQuantityDisplay">
                            <%= order.orderitems.reduce((total, item)=> total + item.quantity, 0)
                                %>
                        </div>
                    </div>

                    <div class="stat-card">
                        <div class="stat-header">
                            <div class="stat-title">Order Total</div>
                            <div class="stat-icon" style="background: rgba(40, 167, 69, 0.1); color: var(--success);">
                                <i class="fas fa-rupee-sign"></i>
                            </div>
                        </div>
                        <div class="stat-value">₹<%= order.totalPrice.toLocaleString("en-IN") %>
                        </div>
                    </div>

                    <div class="stat-card">
                        <div class="stat-header">
                            <div class="stat-title">Payment Status</div>
                            <div class="stat-icon" style="background: rgba(23, 162, 184, 0.1); color: var(--info);">
                                <i class="fas fa-money-bill-wave"></i>
                            </div>
                        </div>
                        <div class="stat-value" style="font-size: 1rem; color: var(--warning);">
                            <%= order.paymentMethod %>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Enhanced Individual Ordered Items Section with Quantity-Based Tracking -->
            <section class="content-section">
                <h3 class="section-title">
                    <i class="fas fa-list"></i>
                    Order Items with Quantity Management
                    <div class="ms-auto">
              
                    </div>
                </h3>

                <!-- Bulk Actions Panel -->
                <div class="bulk-actions" id="bulkActionsPanel">
                    <div class="d-flex justify-content-between align-items-center">
                        <span class="fw-bold">
                            <i class="fas fa-check-circle text-primary me-2"></i>
                            <span id="selectedCount">0</span> item(s) selected
                        </span>
                        <div class="d-flex gap-2">
                            <button class="btn btn-sm btn-primary" onclick="bulkUpdateSelected()">
                                <i class="fas fa-edit"></i> Bulk Update Status
                            </button>
                            <button class="btn btn-sm btn-success" onclick="bulkMarkShipped()">
                                <i class="fas fa-shipping-fast"></i> Mark as Shipped
                            </button>
                            <button class="btn btn-sm btn-info" onclick="bulkMarkDelivered()">
                                <i class="fas fa-check-circle"></i> Mark as Delivered
                            </button>
                        </div>
                    </div>
                </div>

                <div class="table-responsive">
                    <table class="table table-hover">
                        <thead>
                            <tr>
                                <th width="40">
                                
                                </th>
                                <th><i class="fas fa-tag me-2"></i>Product</th>
                                <th><i class="fas fa-sort-numeric-up me-2"></i>Quantities</th>
                                <th><i class="fas fa-rupee-sign me-2"></i>Price</th>
                                <th><i class="fas fa-truck me-2"></i>Status</th>
                              
                            </tr>
                        </thead>
                        <tbody>
                            <% order.orderitems.forEach((item, itemIndex)=> {
                                const cancelledQty = item.cancelledQuantity || 0;
                                const returnedQty = item.returnedQuantity || 0;
                                const activeQty = item.quantity - cancelledQty - returnedQty;
                                %>
                                <tr data-item-id="<%= item._id %>" class="item-row">
                                    <td>
                                        <input type="checkbox" class="item-checkbox" data-item-id="<%= item._id %>">
                                    </td>
                                    <td>
                                        <div class="d-flex align-items-center">
                                            <img src="<%= item.productId.images[0] %>" alt="Product"
                                                class="product-img me-3">
                                            <div>
                                                <div class="product-name">
                                                    <%= item.productId.productName %>
                                                </div>
                                                <small class="text-muted">Item ID: <%= item._id %></small>
                                            </div>
                                        </div>
                                    </td>
                                    <td>
                                        <div class="quantity-breakdown">
                                            <div class="quantity-badges">
                                                <% if (activeQty> 0) { %>
                                                    <span class="quantity-badge badge-active">Active: <%= activeQty %>
                                                    </span>
                                                    <% } %>
                                                        <% if (cancelledQty> 0) { %>
                                                            <span class="quantity-badge badge-cancelled">Cancelled: <%=
                                                                    cancelledQty %></span>
                                                            <% } %>
                                                                <% if (returnedQty> 0) { %>
                                                                    <span
                                                                        class="quantity-badge badge-returned">Returned:
                                                                        <%= returnedQty %>
                                                                    </span>
                                                                    <% } %>
                                            </div>
                                            <small class="text-muted">Total: <%= item.quantity %></small>
                                        </div>
                                    </td>
                                    <td class="fw-bold">₹<%= (item.price * item.quantity).toLocaleString("en-IN") %>
                                    </td>
                                    <td>
                                        <% const statusClass=(item.status || 'pending' ).toLowerCase()==='shipped'
                                            ? 'status-shipped' : (item.status || 'pending' ).toLowerCase()==='delivered'
                                            ? 'status-delivered' : (item.status || 'pending'
                                            ).toLowerCase()==='cancelled' ? 'status-cancelled' : (item.status
                                            || 'pending' ).toLowerCase()==='returned' ? 'status-returned' : (item.status
                                            || 'pending' ).toLowerCase()==='mixed' ? 'status-mixed' : (item.status
                                            || 'pending' ).toLowerCase()==='return request' ? 'status-return-request' :
                                            (item.status || 'pending' ).toLowerCase().includes('partially cancelled')
                                            ? 'status-partially-cancelled' : (item.status || 'pending'
                                            ).toLowerCase().includes('partially returned') ? 'status-partially-returned'
                                            : (item.status || 'pending' ).toLowerCase().includes('partially')
                                            ? 'status-pending' : 'status-processing' ; %>
                                            <span class="status-badge <%= statusClass %>">
                                                <i class="fas fa-circle status-icon"></i>
                                                <%= (item.status || 'PENDING' ).toUpperCase() %>
                                            </span>

                                            <!-- Reason Display -->
                                            <% if (item.cancellationReason && item.cancellationReason !=='none' ) { %>
                                                <i class="fas fa-info-circle message-icon text-danger"
                                                    onclick="showItemReason('<%= item._id %>','<%= item.productId.productName %>','<%= order._id %>', '<%= item.cancellationReason %>','<%= item.cancelledQuantity%>')"
                                                    title="View cancellation reason"></i>
                                                <% } %>
                                                    <% if (item.returnReason && item.returnReason !=='none' ) { %>
                                                        <% if (item.status !=='Returned' && item.status
                                                            !=='Return Rejected' ) { %>
                                                            <i class="fas fa-undo message-icon text-warning"
                                                                onclick="validateReturnReason('<%= item._id %>', '<%= item.productId.productName %>', '<%= item.returnReason %>', '<%= order._id %>', '<%= item.returnedQuantity %>')"
                                                                title="Validate return reason"></i>
                                                            <% } %>
                                                                <% } %>

                                    </td>
                                    
                                </tr>
                                <% }); %>
                        </tbody>
                    </table>
                </div>
            </section>

            <!-- Enhanced Customer Details Section -->
            <section class="content-section">
                <h3 class="section-title">
                    <i class="fas fa-user"></i>
                    Customer Details
                </h3>
                <div class="customer-details-grid">
                    <div class="detail-item">
                        <div class="detail-label">Customer Name</div>
                        <div class="detail-value">
                            <%= order.shippingAddress.title %>
                        </div>
                    </div>
                    <div class="detail-item">
                        <div class="detail-label">Phone Number</div>
                        <div class="detail-value">
                            <%= order.shippingAddress.phone %>
                        </div>
